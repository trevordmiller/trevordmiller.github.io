{
  "ci": {
    "collect": {
      "staticDistDir": ".",
      "settings": {"skipAudits": ["uses-http2"]}
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", {"minScore": 1.00}],
        "categories:accessibility": ["error", {"minScore": 1.00}],
        "categories:best-practices": ["error", {"minScore": 1.00}],
        "categories:seo": ["error", {"minScore": 1.00}],
        "uses-http2": "off"
      }
    }
  }
}
